node
{
    stage('checkout')
    {
        checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[credentialsId: 'trainingport-password-github', url: 'https://github.com/trainingport/cicddemo']]])
    }
    stage('build') 
    {
        bat label: '', script: 'mvn install'
    }
    stage('deploy')
    {
        deploy adapters: [tomcat8(credentialsId: 'tomcat8-deployer', url: 'http://localhost:8080/')], contextPath: 'demoapp', war: '**/*.war'
    }
}
